<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:id="@+id/ScrollView01"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="fill_parent" android:layout_width="fill_parent"
	android:paddingRight="5dp">
	<LinearLayout android:orientation="vertical"
		android:layout_width="fill_parent" android:layout_height="fill_parent"
		android:padding="5dp" android:scrollbars="vertical">

		<LinearLayout android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/LinearLayoutMessage"></LinearLayout>
		<TableLayout android:layout_height="wrap_content" android:id="@+id/TableLayoutDeviceInfo" android:layout_width="match_parent">
			<TableRow android:id="@+id/TableRowTrigger" android:layout_height="wrap_content" android:layout_width="match_parent">
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="5dp"
					android:textSize="18sp" android:id="@+id/labelCurrentTrigger"
					android:text="@+string/labelCurrentTrigger"></TextView>
				<TextView android:text="@+id/TextView01"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:paddingRight="5dp" android:textSize="18sp" android:id="@+id/tvCurrentTrigger"></TextView>
			</TableRow>
			<TableRow android:layout_height="wrap_content" android:id="@+id/TableRowProfile" android:layout_width="match_parent" android:baselineAligned="true">
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="5dp"
					android:textSize="18sp" android:id="@+id/labelCurrentProfile"
					android:text="@+string/labelCurrentProfile"></TextView>
				<Spinner android:layout_height="wrap_content"
					android:layout_width="wrap_content" android:id="@+id/spProfiles" android:layout_weight=".1"></Spinner>
			</TableRow>
			<TableRow android:layout_height="wrap_content" android:id="@+id/TableRowBattery" android:layout_width="match_parent">
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="5dp"
					android:id="@+id/labelCurrentBattery" android:text="@+string/labelCurrentBattery"
					android:textSize="18sp" android:paddingTop="10dp"></TextView>
				<TextView android:text="@+id/TextView01"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:paddingRight="5dp" android:textSize="18sp" android:id="@+id/tvBatteryLevel"></TextView>
			</TableRow>
			<TableRow android:layout_height="wrap_content" android:id="@+id/TableRowPower" android:layout_width="match_parent">
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="5dp"
					android:textSize="18sp" android:id="@+id/labelAcPower"
					android:text="@+string/labelAcPower"></TextView>
				<TextView android:text="@+id/TextView01"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:paddingRight="5dp" android:textSize="18sp" android:id="@+id/tvAcPower"></TextView>
			</TableRow>
			<TableRow android:layout_height="wrap_content" android:id="@+id/TableRowBatteryCurrent" android:layout_width="match_parent">
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="5dp"
					android:textSize="18sp" android:id="@+id/labelBatteryCurrent"
					android:text="@string/labelUsage"></TextView>
				<TextView android:text="@+id/TextView01"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:paddingRight="5dp" android:textSize="18sp" android:id="@+id/tvBatteryCurrent"></TextView>
			</TableRow>
			<TableRow android:layout_height="wrap_content"
				android:id="@+id/tableRowGov" android:layout_width="match_parent">
				<TextView android:paddingTop="10dp" android:textSize="20sp"
					android:layout_width="wrap_content" android:paddingRight="5dp"
					android:text="@string/labelCpuGov" android:id="@+id/textView1"
					android:layout_height="wrap_content" android:layout_gravity="center_vertical"></TextView>
				<Spinner android:layout_height="wrap_content"
					android:layout_width="wrap_content" android:prompt="@string/spinnerPromptGov"
					android:id="@+id/SpinnerCpuGov" android:paddingTop="10dp" android:layout_marginTop="10dp" android:layout_weight=".1"></Spinner>
			</TableRow>
			<TableRow android:layout_height="wrap_content"
				android:layout_width="match_parent" android:id="@+id/tableRowThresholds">
				<TextView android:layout_height="wrap_content"
					android:text="@string/label_governor_tresholds" android:id="@+id/textView2"
					android:layout_width="wrap_content" android:paddingRight="5dp"></TextView>
				<TextView android:layout_width="wrap_content"
					android:paddingRight="7dp" android:text="TextView" android:id="@+id/tvGovTreshholds"
					android:layout_height="wrap_content"></TextView>
			</TableRow>
		</TableLayout>
		<TextView android:layout_height="wrap_content"
			android:paddingTop="5dp" android:text="Explain Gov"
			android:layout_width="wrap_content" android:id="@+id/tvExplainGov"></TextView>


		<TextView android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="@string/labelCpuFreq"
			android:paddingRight="5dp" android:textSize="20sp"
			android:paddingTop="10dp" />
		<TableLayout android:layout_height="wrap_content"
			android:id="@+id/TableLayoutFreq" android:layout_width="fill_parent">
			<TableRow android:layout_height="wrap_content" android:id="@+id/TableRowMaxFreq"
				android:layout_width="fill_parent">


				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="5dp"
					android:text="@+string/labelMax" android:id="@+id/labelCpuFreqMax" android:layout_gravity="center_vertical"></TextView>
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:text="@+id/tvCpuFreqMax"
					android:paddingRight="5dp" android:id="@+id/tvCpuFreqMax" android:layout_gravity="center_vertical"></TextView>
				<SeekBar android:layout_height="wrap_content"
					android:layout_width="fill_parent" android:id="@+id/SeekBarCpuFreqMax"
					android:layout_weight=".3" android:paddingRight="5dp"
					android:paddingBottom="3dp" android:paddingTop="3dp"></SeekBar>

			</TableRow>
			<TableRow android:layout_height="wrap_content" android:id="@+id/TableRowMinFreq"
				android:layout_width="fill_parent">



				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="5dp"
					android:text="@+string/labelMin" android:id="@+id/labelCpuFreqMin" android:layout_gravity="center_vertical"></TextView>
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="5dp"
					android:text="99999" android:id="@+id/tvCpuFreqMin" android:layout_gravity="center_vertical"></TextView>
				<SeekBar android:layout_height="wrap_content"
					android:layout_width="fill_parent" android:id="@+id/SeekBarCpuFreqMin"
					android:layout_weight=".3" android:paddingRight="5dp"
					android:paddingTop="3dp" android:paddingBottom="3dp"></SeekBar>
			</TableRow>
		</TableLayout>
	</LinearLayout>
</ScrollView>