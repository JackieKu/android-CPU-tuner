<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="wrap_content" android:orientation="vertical"
	android:padding="3dp" android:layout_width="fill_parent">
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/tvName"
		android:textStyle="bold" android:textSize="16sp" android:text="profile name"></TextView>
	<TableLayout android:id="@+id/TableLayout01"
		android:layout_height="wrap_content" android:layout_width="fill_parent">
		<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="@string/labelGovernor"
				android:paddingRight="20dp" android:id="@+id/labelGovernor"></TextView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textSize="16sp"
				android:paddingRight="5dp" android:id="@+id/tvGov" android:text="Governor"></TextView>
		</TableRow>
		<TableRow android:id="@+id/TableRow02" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="@string/labelCpuFreq" android:paddingRight="20dp"></TextView>
			<LinearLayout android:id="@+id/ListView01"
				android:layout_width="wrap_content" android:layout_height="wrap_content">

				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:id="@+id/tvFreqMin"
					android:paddingRight="5dp" android:text="500"></TextView>
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textSize="16sp"
					android:paddingRight="5dp" android:text="-"></TextView>
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:id="@+id/tvFreqMax"
					android:paddingRight="5dp" android:text="1000"></TextView>
			</LinearLayout>
		</TableRow>
		<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:paddingRight="5dp"
				android:text="@string/labelServices"></TextView>
			<LinearLayout android:id="@+id/LinearLayout03"
				android:layout_width="wrap_content" android:layout_height="wrap_content">
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="5dp"
					android:text="@string/label3g2g" android:id="@+id/tvMobiledata3G"></TextView>
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="5dp"
					android:id="@+id/tvMobiledataConnection" android:text="@string/labelMobiledataOn"></TextView>
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:id="@+id/tvSync"
					android:paddingRight="5dp" android:text="@string/labelSyncOn"></TextView>
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:id="@+id/tvGPS"
					android:paddingRight="5dp" android:text="@string/labelGpsOn"></TextView>
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="5dp"
					android:id="@+id/tvWifi" android:text="@string/labelWifiOn"></TextView>
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="5dp"
					android:id="@+id/tvBluetooth" android:text="@string/labelBluetoothOn"></TextView>
				<TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/tvAirplaneMode" android:text="@string/labelAirplaneModeOn"></TextView>

			</LinearLayout>
		</TableRow>
	</TableLayout>


</LinearLayout>
