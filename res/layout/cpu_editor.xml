</LinearLayout><LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/llCpuGov" android:padding="5dp" android:orientation="vertical"><TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="@string/labelCpuGov" android:paddingRight="5dp" android:textSize="20sp"/><Spinner android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/SpinnerCpuGov" android:prompt="@string/spinnerPromptGov"></Spinner>
