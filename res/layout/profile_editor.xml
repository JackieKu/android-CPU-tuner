<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:id="@+id/ScrollView01"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:layout_marginBottom="20dp">
	<LinearLayout android:layout_width="fill_parent" android:layout_height="fill_parent"
		android:padding="5dp" android:id="@+id/llTop" android:orientation="vertical">

		<LinearLayout android:layout_height="wrap_content"
			android:id="@+id/LinearLayoutProfile" android:layout_width="fill_parent">
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/labelPowerProfile"
				android:text="@string/labelPowerProfile" android:paddingRight="5dp"
				android:textSize="22sp"></TextView>
			<EditText android:layout_height="wrap_content" android:id="@+id/etName"
				android:layout_width="fill_parent"></EditText>

		</LinearLayout>
		<TextView android:paddingRight="5dp" android:id="@+id/textView2"
			android:text="@string/labelServices" android:paddingTop="10dp"
			android:layout_height="wrap_content" android:layout_width="fill_parent"
			android:textSize="20sp"></TextView>
		<TableLayout android:id="@+id/TableLayoutServices"
			android:layout_height="wrap_content" android:layout_width="wrap_content">
			<TableRow android:layout_height="wrap_content" android:id="@+id/TableRowMobileData3G"
				android:layout_width="wrap_content">
				<TextView android:paddingRight="5dp"
					android:layout_gravity="center_vertical" android:text="@string/labelMobileData32G"
					android:layout_width="wrap_content" android:id="@+id/TextView01"
					android:layout_height="wrap_content"></TextView>
				<Spinner android:layout_height="wrap_content"
					android:layout_width="wrap_content" android:id="@+id/spMobileData3G"></Spinner>
			</TableRow>
			<TableRow android:layout_height="wrap_content"
				android:id="@+id/TableRowMobiledataConnection" android:layout_width="wrap_content">
				<TextView android:paddingRight="5dp"
					android:layout_gravity="center_vertical" android:text="@string/labelMobileDataConn"
					android:layout_width="wrap_content" android:id="@+id/TextView02"
					android:layout_height="wrap_content"></TextView>
				<Spinner android:layout_height="wrap_content"
					android:layout_width="wrap_content" android:id="@+id/spMobileDataConnection"></Spinner>
			</TableRow>
			<TableRow android:layout_height="wrap_content" android:id="@+id/TableRowSync"
				android:layout_width="wrap_content">
				<TextView android:paddingRight="5dp"
					android:layout_gravity="center_vertical" android:text="@string/labelBackgroundSync"
					android:layout_width="wrap_content" android:id="@+id/TextView01"
					android:layout_height="wrap_content"></TextView>
				<Spinner android:layout_height="wrap_content"
					android:layout_width="wrap_content" android:id="@+id/spSync"></Spinner>
			</TableRow>
			<TableRow android:layout_height="wrap_content" android:id="@+id/TableRowGps"
				android:layout_width="wrap_content">
				<TextView android:paddingRight="5dp"
					android:layout_gravity="center_vertical" android:text="@string/labelGPS"
					android:layout_width="wrap_content" android:id="@+id/TextView01"
					android:layout_height="wrap_content"></TextView>
				<Spinner android:layout_height="wrap_content"
					android:layout_width="wrap_content" android:id="@+id/spGps"></Spinner>
			</TableRow>
			<TableRow android:layout_height="wrap_content" android:id="@+id/TableRowWifi"
				android:layout_width="wrap_content">
				<TextView android:paddingRight="5dp"
					android:layout_gravity="center_vertical" android:text="@string/labelWifi"
					android:layout_width="wrap_content" android:id="@+id/TextView01"
					android:layout_height="wrap_content"></TextView>
				<Spinner android:layout_height="wrap_content"
					android:layout_width="wrap_content" android:id="@+id/spWifi"></Spinner>
			</TableRow>
			<TableRow android:layout_height="wrap_content" android:id="@+id/TableRowBluetooth"
				android:layout_width="wrap_content">
				<TextView android:paddingRight="5dp"
					android:layout_gravity="center_vertical" android:text="@string/labelBluetooth"
					android:layout_width="wrap_content" android:id="@+id/TextView01"
					android:layout_height="wrap_content"></TextView>
				<Spinner android:layout_height="wrap_content"
					android:layout_width="wrap_content" android:id="@+id/spBluetooth"></Spinner>
			</TableRow>
		</TableLayout>
		<LinearLayout android:layout_height="wrap_content" android:layout_width="match_parent" android:id="@+id/llGovernorFragmentAncor" android:orientation="vertical"></LinearLayout>
		
		
		
		<TextView android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="@string/labelCpuFreq"
			android:paddingRight="5dp" android:textSize="20sp"
			android:paddingTop="20dp" />
		<TableLayout android:layout_height="wrap_content"
			android:id="@+id/TableLayoutFreq" android:layout_width="fill_parent">
			<TableRow android:layout_height="wrap_content" android:id="@+id/TableRowMaxFreq"
				android:layout_width="fill_parent" android:layout_gravity="center_vertical">



				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="5dp"
					android:text="@+string/labelMax" android:id="@+id/labelCpuFreqMax"></TextView>
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:text="@+id/tvCpuFreqMax"
					android:paddingRight="5dp" android:id="@+id/tvCpuFreqMax"
					android:layout_gravity="right"></TextView>
				<SeekBar android:layout_height="wrap_content"
					android:layout_width="fill_parent" android:id="@+id/SeekBarCpuFreqMax"
					android:layout_weight=".3" android:paddingRight="5dp"></SeekBar>
			</TableRow>
			<TableRow android:layout_height="wrap_content" android:id="@+id/TableRowMinFreq"
				android:layout_width="fill_parent" android:layout_gravity="center_vertical">
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="5dp"
					android:text="@+string/labelMin" android:id="@+id/labelCpuFreqMin"></TextView>
				<TextView android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:paddingRight="5dp"
					android:text="99999" android:id="@+id/tvCpuFreqMin"
					android:layout_gravity="right"></TextView>

				<SeekBar android:layout_height="wrap_content"
					android:layout_width="fill_parent" android:id="@+id/SeekBarCpuFreqMin"
					android:layout_weight=".3" android:paddingRight="5dp"></SeekBar>
			</TableRow>

		</TableLayout>







	</LinearLayout>

</ScrollView>