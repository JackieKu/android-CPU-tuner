<?xml version="1.0" encoding="utf-8"?>
	<ScrollView android:id="@+id/ScrollView01" xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
<LinearLayout 
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:padding="5dp"
	android:scrollbars="vertical">

	<LinearLayout android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/LinearLayoutMessage"></LinearLayout>
	<TableLayout android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/TableLayoutDeviceInfo">
		<TableRow android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/TableRowTrigger">
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:paddingRight="5dp"
				android:textSize="18sp" android:id="@+id/labelCurrentTrigger"
				android:text="@+string/labelCurrentTrigger"></TextView>
			<TextView android:text="@+id/TextView01"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:paddingRight="5dp" android:textSize="18sp" android:id="@+id/tvCurrentTrigger"></TextView>
		</TableRow>
		<TableRow android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/TableRowProfile">
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:paddingRight="5dp"
				android:textSize="18sp" android:id="@+id/labelCurrentProfile"
				android:text="@+string/labelCurrentProfile"></TextView>
			<TextView android:text="@+id/TextView01"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:paddingRight="5dp" android:textSize="18sp" android:id="@+id/tvCurrentProfile"></TextView>
		</TableRow>
		<TableRow android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/TableRowBattery">
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:paddingRight="5dp"
				android:id="@+id/labelCurrentBattery" android:text="@+string/labelCurrentBattery"
				android:textSize="18sp" android:paddingTop="20dp"></TextView>
			<TextView android:text="@+id/TextView01"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:paddingRight="5dp" android:textSize="18sp" android:id="@+id/tvBatteryLevel"></TextView>
		</TableRow>
		<TableRow android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/TableRowPower">
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:paddingRight="5dp"
				android:textSize="18sp" android:id="@+id/labelAcPower" android:text="@+string/labelAcPower"></TextView>
			<TextView android:text="@+id/TextView01"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:paddingRight="5dp" android:textSize="18sp" android:id="@+id/tvAcPower"></TextView>
		</TableRow>
	</TableLayout>
	<LinearLayout android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/llCpuGov"
		android:orientation="vertical">
		<TextView android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="@string/labelCpuGov"
			android:paddingRight="5dp" android:textSize="20sp"
			android:paddingTop="10dp" />
		<Spinner android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/SpinnerCpuGov"
			android:prompt="@string/spinnerPromptGov"></Spinner>
	
</LinearLayout>
	
	
	<TextView android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="@string/labelCpuFreq"
		android:paddingRight="5dp" android:textSize="20sp" android:paddingTop="10dp" /><TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Explain Gov" android:id="@+id/tvExplainGov"></TextView>
	<TableLayout android:layout_height="wrap_content"
		android:id="@+id/TableLayoutFreq" android:layout_width="fill_parent">
		<TableRow android:layout_height="wrap_content" android:id="@+id/TableRowMaxFreq"
			android:layout_width="fill_parent">
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:paddingRight="5dp" android:text="@+string/labelMax" android:id="@+id/labelCpuFreqMax"></TextView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="@+id/tvCpuFreqMax"
				android:paddingRight="5dp" android:id="@+id/tvCpuFreqMax"
				android:layout_gravity="right"></TextView>
			<SeekBar android:layout_height="wrap_content"
				android:layout_width="fill_parent" android:id="@+id/SeekBarCpuFreqMax"
				android:layout_weight=".3"></SeekBar>
		</TableRow>
		<TableRow android:layout_height="wrap_content" android:id="@+id/TableRowMinFreq"
			android:layout_width="fill_parent">
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:paddingRight="5dp"
				android:text="@+string/labelMin" android:id="@+id/labelCpuFreqMin"></TextView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:paddingRight="5dp"
				android:text="99999" android:id="@+id/tvCpuFreqMin"
				android:layout_gravity="right"></TextView>
			<SeekBar android:layout_height="wrap_content"
				android:layout_width="fill_parent" android:id="@+id/SeekBarCpuFreqMin"
				android:layout_weight=".3"></SeekBar>
		</TableRow>
	</TableLayout>
</LinearLayout>
</ScrollView>