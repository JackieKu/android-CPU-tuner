<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:title="@string/labelSettingsTab"
>
	<CheckBoxPreference
		android:key="prefKeyEnableProfiles"
		android:defaultValue="true"
		android:title="@string/prefEnableCputuner"
		android:summaryOff="@string/prefEnableCputunerOff"
		android:summaryOn="@string/prefEnableCputunerOn"
	></CheckBoxPreference>
	<PreferenceScreen
		android:title="@string/prefConfigurations" 
		android:key="prefKeyConfigurations"
		android:summary="@string/prefConfigurationsLong"
	>
		<PreferenceCategory
			android:title="@string/prefConfigurations"
			android:key="prefKeyConfigurations"
			android:summary="@string/prefConfigurationsLong"
		>
			<PreferenceScreen
				android:key="prefKeyConfigurationsManage"
				android:title="@string/prefConfigurationsManage"
				android:summary="@string/prefConfigurationsManageLong"
			></PreferenceScreen>
			<PreferenceScreen
				android:key="prefKeyConfigurationsAutoLoad"
				android:title="@string/prefConfigurationsAutoLoad"
				android:summary="@string/prefConfigurationsAutoLoadLong"
			></PreferenceScreen>
		</PreferenceCategory>
		<PreferenceCategory
			android:key="prefkeyConfigurationsSettings"
			android:title="@string/prefConfigurationsSettings"
		>
			<CheckBoxPreference
				android:title="@string/prefSaveConfigOnSwitch"
				android:key="prefKeySaveConfigOnSwitch"
				android:summary="@string/prefSaveConfigOnSwitchFull"
				android:defaultValue="true"
			></CheckBoxPreference>
		</PreferenceCategory>
	</PreferenceScreen>
	<PreferenceScreen
		android:key="prefKeyGuiScreen"
		android:title="@string/prefCatGUI"
		android:summary="@string/prefCatGUILong"
	>
		<PreferenceCategory
			android:title="@string/prefCatDisplay"
			android:key="prefkeyCatDisplay"
		>
			<ListPreference
				android:title="@string/prefLanguage"
				android:key="prefKeyLanguage"
				android:entryValues="@array/prefLanguageValues"
				android:entries="@array/prefLanguageEntries"
				android:summary="@string/prefLanguageLong"
			></ListPreference>
			<Preference
				android:title="@string/prefUserLevel"
				android:key="prefKeyUserLevel"
				android:summary="@string/prefUserLevelLong"
			></Preference>
			<!-- <ListPreference android:title="@string/prefUserLevel"
				android:key="prefKeyUserLevel" android:summary="@string/prefUserLevelLong"
				android:entryValues="@array/prefUserLevelValues" android:entries="@array/prefUserLevelEntries"></ListPreference> -->
			<ListPreference
				android:title="@string/prefKeyCalcPowerUsage"
				android:summary="@string/prefKeyCalcPowerUsageLong"
				android:defaultValue="1"
				android:entries="@array/prefCalcPowerUsageEntries"
				android:key="prefKeyCalcPowerUsageType"
				android:entryValues="@array/prefCalcPowerUsageValues"
			></ListPreference>
			<EditTextPreference
				android:summary="@string/prefProfileSwitchLogSizeLong"
				android:title="@string/prefProfileSwitchLogSize"
				android:defaultValue="10"
				android:key="prefKeyProfileSwitchLogSize"
			></EditTextPreference>
		</PreferenceCategory>
		<PreferenceCategory
			android:title="@string/prefCatNotifications"
			android:key="prefKeyCatNotifications"
		>
			<CheckBoxPreference
				android:key="prefKeyStatusbarAddTo"
				android:dependency="prefKeyEnableProfiles"
				android:defaultValue="true"
				android:title="@string/prefStatusbarAddTo"
				android:summaryOff="@string/prefStatusbarAddToOff"
				android:summaryOn="@string/prefStatusbarAddToOn"
			></CheckBoxPreference>
			<CheckBoxPreference
				android:key="prefKeyStatusbarNotifications"
				android:title="@string/prefStatusbarNotifications"
				android:dependency="prefKeyStatusbarAddTo"
				android:summaryOff="@string/prefStatusbarNotificationsOff"
				android:summaryOn="@string/prefKeyEnableProfilesOn"
				android:defaultValue="false"
			></CheckBoxPreference>

		</PreferenceCategory>

	</PreferenceScreen>
	<PreferenceScreen
		android:title="@string/prefCatSystem"
		android:key="prefKeySystemScreen"
		android:summary="@string/prefCatSystemLong"
	>
		<PreferenceScreen
			android:key="prefKeyProfiles"
			android:title="@string/prefKeyProfiles"
			android:summary="@string/prefKeyProfilesLong"
		>

			<CheckBoxPreference
				android:title="@string/prefCallInProgressProfile"
				android:key="prefKeyCallInProgressProfile"
				android:summary="@string/prefCallInProgressProfileLong"
				android:enabled="true"
				android:defaultValue="true"
			></CheckBoxPreference>
			<CheckBoxPreference
				android:key="prefKeyUseVirtualGovernors"
				android:summary="@string/prefUseVirtualGovernorsLong"
				android:title="@string/cbEnableVirtualGovernors"
				android:defaultValue="true"
			></CheckBoxPreference>
			<CheckBoxPreference
				android:key="prefKeySwitchProfileWhilePhoneNotIdle"
				android:summaryOff="@string/prefSwitchProfileWhilePhoneNotIdleOff"
				android:summaryOn="@string/prefSwitchProfileWhilePhoneNotIdleOn"
				android:title="@string/prefSwitchProfileWhilePhoneNotIdle"
				android:defaultValue="false"
			></CheckBoxPreference>
			<EditTextPreference
				android:title="@string/prefBatteryHotTemp"
				android:key="prefKeyBatteryHotTemp"
				android:dialogMessage="@string/prefBatteryHotTempMsg"
				android:dialogTitle="@string/prefBatteryHotTemp"
				android:summary="@string/prefBatteryHotTempLong"
			></EditTextPreference>

		</PreferenceScreen>
		<PreferenceScreen
			android:key="prefKeyServiceSwitches"
			android:title="@string/prefServiceSwitches"
			android:summary="@string/prefServiceSwitchesLong"
		>
			<PreferenceCategory
				android:key="prefKeyServiceSwitches"
				android:title="@string/prefServiceSwitches"
			>
				<CheckBoxPreference
					android:key="prefKeyAllowManualServiceChanges"
					android:title="@string/prefAllowManualServiceChanges"
					android:summaryOff="@string/prefAllowManualServiceChangesOff"
					android:summaryOn="@string/prefAllowManualServiceChangesOn" android:defaultValue="false"></CheckBoxPreference>
				<ListPreference
					android:title="@string/prefNetworkModeOnWifiConnected"
					android:summary="@string/prefNetworkModeOnWifiConnectedLong"
					android:key="prefKeyNetworkModeOnWifiConnected"
					android:entryValues="@array/mobiledataOnWifiStatesValues"
					android:entries="@array/mobiledataOnWifiStatesEnties"
					android:defaultValue="0"
				></ListPreference>
				<CheckBoxPreference
					android:key="prefKeySwitchWifiOnConnectedNetwork"
					android:summaryOff="@string/prefSwitchWifiOnConnectedNetworkOff"
					android:summaryOn="@string/prefSwitchWifiOnConnectedNetworkOn"
					android:title="@string/prefSwitchWifiOnConnectedNetwork" android:defaultValue="true"></CheckBoxPreference>



			</PreferenceCategory>
			<PreferenceCategory android:title="@string/prefCatPulsedServiceSwitches">
				<EditTextPreference
					android:summary="@string/prefPulseDelayOnLong"
					android:defaultValue="1"
					android:key="prefKeyPulseDelayOn"
					android:title="@string/prefPulseDelayOn"
				></EditTextPreference>
				<EditTextPreference
					android:summary="@string/prefPulseDelayOffLong"
					android:defaultValue="30"
					android:key="prefKeyPulseDelayOff"
					android:title="@string/prefPulseDelayOff"
				></EditTextPreference>
				<CheckBoxPreference
					android:title="@string/prefPulseMobiledataOnWifi"
					android:key="prefKeyPulseMobiledataOnWifi"
					android:summaryOn="@string/prefPulseMobiledataOnWifiOn"
					android:summaryOff="@string/prefPulseMobiledataOnWifiOff"
					android:defaultValue="true"
				></CheckBoxPreference>
			</PreferenceCategory>
		</PreferenceScreen>




		<PreferenceScreen
			android:title="@string/prefCpu"
			android:key="prefKeyCpu"
			android:summary="@string/prefCpuLong"
		>
			<ListPreference
				android:entries="@array/prefMulticoreEntries"
				android:key="prefKeyMulticore"
				android:entryValues="@array/prefMulticoreValues"
				android:summary="@string/prefMulticoreLong"
				android:title="@string/prefMulticore"
				android:defaultValue="2"
			></ListPreference>
			<CheckBoxPreference
				android:title="@string/prefEnableUserspaceGovernor"
				android:defaultValue="false"
				android:key="prefKeyEnableUserspaceGovernor"
				android:summaryOff="@string/prefEnableUserspaceGovernorOff"
				android:summaryOn="@string/prefEnableUserspaceGovernorOn"
			></CheckBoxPreference>
			<EditTextPreference
				android:defaultValue="400"
				android:summary="@string/prefMinSensibleFrequencyLong"
				android:key="prefKeyMinSensibleFrequency"
				android:title="@string/prefMinSensibleFrequency"
			></EditTextPreference>
			<EditTextPreference
				android:summary="@string/prefCpuFreqLong"
				android:key="prefKeyCpuFreq"
				android:title="@string/prefCpuFreq"
				android:dialogMessage="@string/prefCpuFreqMsg"
			></EditTextPreference>
			<Preference
				android:key="prefKeyCapabilities"
				android:title="@string/prefCapabilities"
				android:summary="@string/prefCapabilitiesLong"
			></Preference>

			<PreferenceCategory
				android:title="@string/prefKeyFrequencies"
				android:key="prefKeyDefaultFrequencies"
			>
				<ListPreference
					android:title="@string/prefMaxFreq"
					android:key="prefKeyMaxFreq"
					android:summary="@string/prefMaxFreqLong"
					android:defaultValue="-1"
				></ListPreference>
				<ListPreference
					android:defaultValue="-1"
					android:title="@string/prefMinFreq"
					android:key="prefKeyMinFreq"
					android:summary="@string/prefMinFreqLong"
				></ListPreference>
			</PreferenceCategory>
		</PreferenceScreen>
		<EditTextPreference
			android:key="prefKeyEnableBeta"
			android:title="@string/prefEnableBeta"
			android:dialogMessage="@string/prefEnableBetaMessage"
			android:dialogTitle="@string/prefEnableBeta"
			android:summary="@string/prefEnableBetaMessage"
		></EditTextPreference>
	</PreferenceScreen>




	<Preference
		android:title="@string/prefBuyMeABeer"
		android:key="prefKeyBuyMeABeer"
		android:summary="@string/prefBuyMeABeerLong"
	></Preference>
	<PreferenceScreen
		android:title="@string/prefVarious"
		android:key="prefKeyVarious"
		android:summary="@string/prefVariousLong"
	>
		<Preference android:key="prefKeyVersion"></Preference>
		<Preference
			android:key="prefKeyChangelog"
			android:title="@string/prefChangelog"
		></Preference>
		<PreferenceScreen
			android:title="@string/prefScreenLegal"
			android:key="prefKeyLegal"
		>

			<Preference
				android:key="prefKeyLegalGnomeIcons"
				android:summary="@string/prefLegalGnomeIconsLong"
				android:title="@string/prefLegalGnomeIcons"
			></Preference>
			<Preference
				android:summary="@string/prefLegalServiceIconsLong"
				android:title="@string/prefLegalServiceIcons"
			android:key="prefKeyLegalServiceIcons"></Preference>
			<!-- <Preference android:key="prefKeyLegalOxigenIcons"
				android:title="@string/prefLegalOxigenIcons" android:summary="@string/prefLegalOxigenIconsLong"></Preference>
			-->
		</PreferenceScreen>

	</PreferenceScreen>

</PreferenceScreen>
