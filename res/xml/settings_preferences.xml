<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

	<PreferenceCategory android:title="Basics"
		android:key="prefKeyBasics">
		<PreferenceScreen android:key="prefKeyProfiles" android:title="@string/prefKeyProfiles"><PreferenceCategory android:key="prefKeyProfiles" android:title="@string/prefKeyProfiles"><ListPreference android:title="@string/prefKeyCalcPowerUsage" android:summary="@string/prefKeyCalcPowerUsageLong" android:defaultValue="1" android:entries="@array/prefCalcPowerUsageEntries" android:key="prefKeyCalcPowerUsageType" android:entryValues="@array/prefCalcPowerUsageValues"></ListPreference><CheckBoxPreference android:title="@string/prefEnableProfiles" android:summaryOn="@string/prefEnableProfilesOn" android:key="prefKeyEnableProfiles" android:defaultValue="true" android:summaryOff="@string/prefEnableProfilesOff"></CheckBoxPreference><CheckBoxPreference android:key="applyCpuSettingsOnBoot" android:title="@string/prefApplyOnBoot" android:summaryOn="@string/prefApplyOnBootOn" android:summaryOff="@string/prefApplyOnBootOff" android:defaultValue="true"></CheckBoxPreference><Preference android:key="prefKeyResetToDefault" android:summary="@string/prefResetToDefaultLong" android:title="@string/prefResetToDefault" android:enabled="true"></Preference>
</PreferenceCategory>
</PreferenceScreen>
		
		<PreferenceScreen android:title="@string/prefCatNotifications"
			android:key="prefKeyNotificationScreen">




			<PreferenceCategory android:title="@string/prefCatNotifications">
				<CheckBoxPreference android:key="prefKeyStatusbarAddTo"
					android:dependency="prefKeyEnableProfiles" android:defaultValue="true"
					android:title="@string/prefStatusbarAddTo" android:summaryOff="@string/prefStatusbarAddToOff"
					android:summaryOn="@string/prefStatusbarAddToOn"></CheckBoxPreference>
				<CheckBoxPreference android:key="prefKeyStatusbarNotifications"
					android:defaultValue="true" android:title="@string/prefStatusbarNotifications"
					android:dependency="prefKeyStatusbarAddTo" android:summaryOff="@string/prefStatusbarNotificationsOff"
					android:summaryOn="@string/prefKeyEnableProfilesOn"></CheckBoxPreference>
				<CheckBoxPreference android:key="prefKeyToastNotifications"
					android:title="@string/prefToastNotifications"
					android:defaultValue="false"></CheckBoxPreference>
			</PreferenceCategory>
		</PreferenceScreen>
	
</PreferenceCategory>
	<PreferenceCategory android:title="@string/prefCatSystem"
		android:key="prefKeySystemCat">
		
		<PreferenceScreen android:title="@string/prefCatSystem"
			android:key="prefKeySystemScreen">


			<PreferenceCategory android:title="@string/prefCatSystem"
				android:key="prefKeySystemCat"><Preference android:key="prefKeyCapabilities" android:title="@string/prefCapabilities" android:summary="@string/prefCapabilitiesLong"></Preference>
				
				<CheckBoxPreference android:key="prefKeyAllowManualServiceChanges" android:title="@string/prefAllowManualServiceChanges" android:summary="@string/prefAllowManualServiceChangesLong" android:defaultValue="true"></CheckBoxPreference><ListPreference android:title="@string/prefUserLevel" android:key="prefKeyUserLevel" android:summary="@string/prefUserLevelLong" android:entryValues="@array/prefUserLevelValues" android:entries="@array/prefUserLevelEntries"></ListPreference><EditTextPreference android:defaultValue="400" android:summary="@string/prefMinSensibleFrequencyLong" android:key="prefKeyMinSensibleFrequency" android:title="@string/prefMinSensibleFrequency"></EditTextPreference>
				<CheckBoxPreference android:key="prefKeySystemApp"
					android:summaryOn="@string/prefSystemAppOn" android:title="@string/prefSystemApp"
					android:defaultValue="false" android:summaryOff="@string/prefSystemAppOff"></CheckBoxPreference>
				<EditTextPreference android:summary="@string/prefCpuFreqLong" android:key="prefKeyCpuFreq"
					android:title="@string/prefCpuFreq" android:dialogMessage="@string/prefCpuFreqMsg"></EditTextPreference>
			



</PreferenceCategory>

		</PreferenceScreen>
	</PreferenceCategory>

	


<PreferenceCategory android:title="@string/prefVarious" android:key="prefKeyVarious"><Preference android:title="@string/prefBuyMeABeer" android:key="prefKeyBuyMeABeer"></Preference><EditTextPreference android:key="prefKeyEnableBeta" android:title="@string/prefEnableBeta" android:dialogMessage="@string/prefEnableBetaMessage" android:dialogTitle="@string/prefEnableBeta" android:summary="@string/prefEnableBetaLong"></EditTextPreference></PreferenceCategory>
</PreferenceScreen>
